<script type="text/javascript">

jQuery.noConflict()(function($){

$(document).ready(function ()

{ // after loading the DOM

    $("#frmRequestQuote").submit(function ()

    {

        // this points to our form

        var str = $(this).serialize(); // Serialize the data for the POST-request

        $.ajax(

        {

            type: "POST",

            url: 'http://www.riaxe.com/wp-content/themes/riaxe/request_quote.php',

			fileElementId:'attachment',

            data: str,

            success: function (msg)

            {

                $("#msgnote").ajaxComplete(function (event, request, settings)

                {

                    if (msg == 'OK')

                    {

                        result = '<div class="notification_ok">Message was sent to website administrator, thank you!</div>';

                        //$("#fields").hide();

                    }

                    else

                    {

                        result = msg;

                    }

                    $(this).html(result);

                });

            }

        });

        return false;

    });

});

});

</script>

